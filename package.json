{
  "name": "stop-mess-around",
  "description": "防摸鱼的chrome插件: 通过强制的手段禁止大家浪费时间摸鱼，在上班/学习期间下意识的打开摸鱼网站, 自动检测摸鱼网站, 提示激励信息后, 关闭摸鱼网站, 滚去学习，滚去做对未来有益的事情！",
  "version": "1.0.6",
  "publisher": "OBKoro1",
  "homepage": "https://github.com/OBKoro1/stop-mess-around",
  "license": "MIT",
  "author": {
    "name": "OBKoro1",
    "email": "obkoro1@foxmail.com",
    "url": "http://obkoro1.com/web_accumulate/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OBKoro1/stop-mess-around"
  },
  "bugs": {
    "url": "https://github.com/OBKoro1/stop-mess-around/issues",
    "email": "obkoro1@foxmail.com"
  },
  "scripts": {
    "serve": "node deploy.js development && vue-cli-service build-watch",
    "build-plugin": "node deploy.js production  && vue-cli-service build",
    "build-release": "node deploy.js development && vue-cli-service build --mode development",
    "lint": "eslint --ext .js,.vue src --fix",
    "prepare": "husky install",
    "analyze": "npm run build --report"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.8.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "vue-cli-plugin-chrome-ext": "0.0.5",
    "vue-template-compiler": "^2.5.21",
    "webpack-bundle-analyzer": "^4.4.2",
    "zip-webpack-plugin": "^3.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
